    "#Problem 2\n",
    "\n",
    "import numpy as np\n",
    "T=float(input(\"Input the Time\"))\n",
    "G=6.67e-11\n",
    "M=5.97e24\n",
    "R=6371\n",
    "a=(G*M/4)\n",
    "b=(T/np.pi)**2\n",
    "h=(a*b)**(1/3)-R\n",
    "print(\"Altitude\",h)"
   ]
  }
